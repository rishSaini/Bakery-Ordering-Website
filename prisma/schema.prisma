datasource db {
  provider = "postgresql"
}

generator client {
  provider = "prisma-client-js"
}

model Inquiry {
  id        String   @id @default(cuid())
  name      String
  email     String
  phone     String?
  eventDate DateTime?
  message   String
  createdAt DateTime @default(now())
}

model GalleryImage {
  id        String   @id @default(cuid())
  title     String?
  imageUrl  String   // Cloudinary URL
  createdAt DateTime @default(now())
}
